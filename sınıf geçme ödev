import java.util.Scanner;

public class OrtalamaHesap {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 int matematik,fizik,kimya,türkçe,müzik;
		    
		    Scanner inp= new Scanner(System.in);
		    
		    System.out.println("matematik notunu giriniz:");
		    matematik= inp.nextInt();
		    if((matematik<=0)||(matematik>100)){
		        matematik=0;
		    }
		    
		    System.out.println("fızık notunu giriniz:");
		    fizik= inp.nextInt();
		    if((fizik<=0)||(fizik>100)) {
		    	fizik=0;
		    }
		    
		    System.out.println("kımya notunu giriniz:");
		    kimya= inp.nextInt();
		    if((kimya<=0)||(kimya>100)) {
		    	kimya=0;
		    }
		    
		    System.out.println("türkçe notunu giriniz:");
		    türkçe= inp.nextInt();
		    if((türkçe<=0)||(türkçe>100)) {
		    türkçe=0;
		    }
		    System.out.println("muzik notunu giriniz:");
		    müzik= inp.nextInt();
		    if((müzik<=0)||(müzik>100)) {
		    	müzik=0;
		    }
		 
		    
		  
		    double ortalama = (matematik+fizik+kimya+türkçe+müzik)/5;
		  
		   if( ortalama<=55) {
			   System.out.print("sınıfta kaldınız");
			   System.out.print("ortalamanız:"+ ortalama);

		   }
		   else {
			   System.out.print("sınıfı geçtiniz");
			   System.out.print("ortalamanız:"+ ortalama);

		   }

	}

}
